{% extends './layouts/layout.njk' %}

{% block pageTitle %}{{ serviceName }}{% endblock %}

{% block content %}
<div class="govuk-grid-row">
  <div class="govuk-grid-column-two-thirds">
    <h1 class="govuk-heading-l">Name: {{token.firstName}} {{token.lastName}}</h1>
    <p class="govuk-body">Email: {{token.email}}</p>
    <p class="govuk-body">TokenExpiry: {{tokenExpiry}}</p>
{% endblock %}